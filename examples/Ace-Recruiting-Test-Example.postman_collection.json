    "_exporter_id": "25409651"
  },
  "item": [
    {
      "name": "Health Check",
      "item": [
        {
          "name": "Is Controller Alive",
          "request": {
            "auth": { "type": "noauth" },
            "method": "GET",
            "header": [],
            "url": { "raw": "{{baseUrl}}/api/public/hello", "host": ["{{baseUrl}}"], "path": ["api","public","hello"] }
          },
          "response": []
        }
      ]
    },
    {
