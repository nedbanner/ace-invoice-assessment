            "body": {
              "mode": "raw",
              "raw": "{\n  \"invoiceData\": {\n    \"invoiceDate\": \"2024-12-20T14:30:00Z\",\n    \"customerId\": \"AA5FD07A-05D6-460F-B8E3-6A09142F9D71\"\n  },\n  \"products\": [\n    {\n      \"productId\": \"26812D43-CEE0-4413-9A1B-0B2EABF7E92C\",\n      \"quantity\": 2\n    },\n    {\n      \"productId\": \"3C85F645-CE57-43A8-B192-7F46F8BBC273\",\n      \"quantity\": 5\n    }\n  ]\n}"
            },
            "url": { "raw": "{{baseUrl}}/api/order/new", "host": ["{{baseUrl}}"], "path": ["api","order","new"] }
          },
          "response": []
